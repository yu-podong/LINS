<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="300dp"
    android:layout_height="300dp"
    android:orientation="vertical"
    android:gravity="center"
    android:background="#9E9D9D9D"
    tools:context=".Scheduler.SchepopActivity">


    <LinearLayout
        android:id="@+id/shcedule"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:padding="20dp"
        android:orientation="vertical"
        android:background="#F9CFC6">

        <TextView
            android:id="@+id/schepop_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginBottom="20dp"
            android:text="날짜"
            android:textSize="25dp"
            android:textStyle="bold"
            android:textColor="@color/brightModeFont"
            />

        <ListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/sche_listview"/>

    </LinearLayout>


</LinearLayout>